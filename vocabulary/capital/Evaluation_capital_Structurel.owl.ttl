ques:capital-1001
    a owl:Class ;
    rdfs:label "Capital Actionnaire" ;
    rdfs:range [
        owl:oneOf rdf:nil
    ] .

ques:capital-1001-user-prop
    a owl:ObjectProperty ;
    rdfs:domain ques:User ;
    rdfs:range ques:capital-1001 .

ques:capital-1002
    a owl:ObjectProperty ;
    rdfs:comment "Veuillez sélectionner une seule des propositions suivantes :" ;
    rdfs:domain ques:capital-1001 ;
    rdfs:label "Action de l’actionnariat pour les affaires de l’entreprise" ;
    rdfs:range [
        owl:oneOf ([
                ques:value 1 ;
                rdfs:label "L’actionnariat joue un rôle négatif pour l’entreprise dans l’un des domaines suivants : levée de fonds, apport d’affaires, partenariat stratégique, recrutement d’hommes clés"
            ]
            [
                ques:value 2 ;
                rdfs:label "L’actionnariat n’aide pas l’entreprise"
            ]
            [
                ques:value 3 ;
                rdfs:label "L’aide que l’actionnariat peut apporter à l’entreprise est très limitée"
            ]
            [
                ques:value 4 ;
                rdfs:label "L’actionnariat a déjà joué un rôle dans l’un des domaines suivants : levée de fonds, apport d’affaires, partenariats stratégiques, recrutement d’hommes clés"
            ]
            [
                ques:value 5 ;
                rdfs:label "L’actionnariat joue un rôle de tout premier plan pour l’entreprise dans l’un des domaines suivants : levée de fonds, apport d’affaires, partenariats stratégiques, recrutement d’hommes clés"
            ]
        )
    ] .

