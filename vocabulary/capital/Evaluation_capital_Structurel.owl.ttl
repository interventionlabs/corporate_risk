ques:capital-1005
    a owl:ObjectProperty ;
    rdfs:comment "Veuillez sélectionner une seule des propositions suivantes :" ;
    rdfs:domain ques:capital-1001 ;
    rdfs:label "Fidélité de l’actionnariat" ;
    rdfs:range [
        owl:oneOf ([
                ques:value 1 ;
                rdfs:label "Actionnariat très volatile : les actionnaires tournent à 80 % tous les 3 à 5 ans voire moins"
            ]
            [
                ques:value 2 ;
                rdfs:label "Actionnariat volatile : le noyau stable représente moins de 35 % du capital et peut évoluer sur une période de 5 ans"
            ]
            [
                ques:value 3 ;
                rdfs:label "Actionnariat moyennement fidèle: 35 % du capital est détenu par les mêmes actionnaires depuis plus de 5 ans et projection à 5 ans analogue"
            ]
            [
                ques:value 4 ;
                rdfs:label "Actionnariat fidèle : plus de 50 % du capital est détenu par les mêmes actionnaires depuis plus de 5 ans et projection à 5 ans analogue"
            ]
            [
                ques:value 5 ;
                rdfs:label "Actionnariat très stable: 80 % du capital est détenu par les mêmes actionnaires depuis plus de 5 ans et projection à 5 ans analogue"
            ]
