@prefix ques: <http://www.bizinnov.com/ontologies/quest.owl.ttl#> .
@prefix      : <http://www.bizinnov.com/ontologies/quest.owl.ttl#> .

{ ?Q ques:header ?L ;
     ques:item ?LAB .
} => {
  ?ENUM rdfs:label ?LAB ;
        :classForOneOf ?Q .
} .

{ ?BASE_URI :newURI ?NEW } <= {
  # (1000000) prolog:random ?RAND .
  ( ?BASE_URI "-user-prop" ) string:concatenation ?NEW_URI .
  ?NEW log:uri ?NEW_URI .
}.


