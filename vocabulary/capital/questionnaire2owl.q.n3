@prefix list: <http://www.w3.org/2000/10/swap/list#>.
@prefix eu: <http://eulersharp.sourceforge.net/2003/03swap/log-rules#>.
@prefix string: <http://www.w3.org/2000/10/swap/string#>.
@prefix fn:     <http://www.w3.org/2005/xpath-functions#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:  <http://www.w3.org/2002/07/owl#>.
@prefix log:  <http://www.w3.org/2000/10/swap/log#>.
@prefix ques: <http://www.bizinnov.com/ontologies/quest.owl.ttl#> .
@prefix      : <http://www.bizinnov.com/ontologies/quest.owl.ttl#> .

# lancer avec:
# eye --nope Evaluation_capital_opérationnel.ttl questionnaire2owl.n3 --query questionnaire2owl.q.n3 > Evaluation_capital_opérationnel.owl.ttl

# Chaque question du fichier Markdown devient une owl:DatatypeProperty à valeurs énumérées
{ ?Q ques:header ?L ;
     ques:paragraph ?C .
 } => {
  ?Q a owl:ObjectProperty ;
    rdfs:label ?L ;
    rdfs:comment ?C .
}.

{ ?Q ques:header ?L .
  _:d eu:findall ( ?ENUM
       { ?ENUM :classForOneOf ?Q }
       ?LIST
  )
} => {
   ?Q rdfs:range [ owl:oneOf ?LIST ] .
} .

{ ?ENUM rdfs:label ?LAB .
 } => {
  ?ENUM rdfs:label ?LAB .
} .


# Rassembler via ques:subheader pour faire une owl:Class, la propriété correspondante de :User, et contribution au FormGroup
{ ?CLASS ques:subheader ?PROP ;
	 ques:header ?LAB .
  ?PROP ques:item ?ITEM .
  ?FORM_GROUP a :FormGroup ;
} => {
  ?CLASS a owl:Class ; rdfs:label ?LAB .
  ?P a owl:ObjectProperty; rdfs:domain :User; rdfs:range ?CLASS .
  ?PROP rdfs:domain ?CLASS.
  ?FORM_GROUP :properties ?P .
}.

